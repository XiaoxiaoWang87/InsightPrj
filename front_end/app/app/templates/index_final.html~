<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script src="http://netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
<script src="../static/assets/js/modernizr.custom.72241.js"></script>
<!-- Custom template scripts -->
<script src="../static/assets/js/magister.js"></script>


    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="static/js/jquery.js"></script>
    <script src="static/dist/js/bootstrap.min.js"></script>

    <script type=text/javascript>
      $SCRIPT_ROOT = {{ request.script_root|tojson|safe }};
    </script>

    <script type=text/javascript>
      var probability = 9;

      $(function() {
        var submit_form = function(e) {
            $.getJSON($SCRIPT_ROOT + '/regression', {
            d: $('#PlayerName').val()
          }, function(data) {
             $('#pphoto').attr("src", data.result[0]);
             probability = data.result[1];
             
 	     $('#pPTS').attr("src", data.result[2]);
             $('#pAST').attr("src", data.result[3]);
             $('#pREB').attr("src", data.result[4]);
             $('#pMIN').attr("src", data.result[5]);
             $('#pFG_PCT').attr("src", data.result[6]);
             $('#pEFF').attr("src", data.result[7]);
       
 	     $('#playername').html("Player:    " + $('#PlayerName').val());

             $('#ill_points').html("Points: The number of points scored by a player.");
             $('#ill_assists').html("Assists: The number of assists -- passes that lead directly to a made basket -- by a player.");
             $('#ill_rebounds').html("Rebounds: The total number of rebounds, including both offensive and defensive rebounds, collected by a player.");
 	     $('#ill_minutes').html("Minutes: The number of minutes a player has played.");
	     $('#ill_percentage').html("Field Goal Percentage: The percentage of attempted field goals (including 2 pointers and 3 pointers) that are made. ");
	     $('#ill_efficiency').html("Efficiency Rating: The NBA's Efficiency Rating is a single number measure of a player's overall contribution (both positive and negative) to a game he plays in. Efficiency = ((Points + Rebounds + Assists + Steals + Blocks) - ((Field Goals Att. - Field Goals Made) + (Free Throws Att. -Free Throws Made) + Turnovers))");

        	if(probability >= 90) {
        	     $('#comments').html("He could be the next Michael Jordan!");
        	}
        	else if(probability>=80){
        	     $('#comments').html("He has a lot of potential to become a super star!");
        	} 
        	else if(probability>=50){
        	     $('#comments').html("There is a fair amount of chance for him to become a super star.");
        	}
        	else if(probability>=33){
        	     $('#comments').html("His chance of becoming future NBA super star is low.");
        	}
        	else {
        	     $('#comments').html("It is very risky to invest on this player!");
        	}


          });
          return false;
        };
    

        $('#GO').bind('click', submit_form);

        $( "#GO" ).click(function() {
          $( "#Page1" ).animate({
            opacity: 0.25,
            left: "+=50",
            height: "toggle"
          }, 500, function() {
            // Animation complete.
          });

          $( "#GO" ).animate({
            opacity: 0.25,
            left: "+=50",
            height: "toggle"
          }, 500, function() {
            // Animation complete.
          });

        });


      });
    </script>

    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["gauge"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {

        var data = google.visualization.arrayToDataTable([
          ['Label', 'Value'],
          ['Likelihood', 0],
        ]);

        var options = {
          width: 800, height: 240,
          redFrom: 0, redTo: 33,
          yellowFrom:33, yellowTo: 67,
          greenFrom: 67, greenTo: 100,
          minorTicks: 5,
          animation:{
            duration: 3000,
            easing: 'linear',
          },
        };

        var chart = new google.visualization.Gauge(document.getElementById('chart_div'));

        chart.draw(data, options);

        setInterval(function() {
          data.setValue(0, 1, probability);
          chart.draw(data, options);
        }, 3000);


      }
    </script>


